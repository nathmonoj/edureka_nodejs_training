< !DOCTYPE html>
  <html>

  <head>
    <script src="https://cdn.tailwindcss.com"></script>
  </head>

  <body>
    <h1 class="bg-blue-500 text-white p-4 rounded-lg">User list</h1>
    <ul class="mt-4">
      <% employees.forEach((employee)=>{ %>
        <li class="bg-gray-200 p-2 my-2 rounded-md">
          <span>
            Name: <%= employee.name %>,
          </span>
          <span>
            Id: <%= employee.id %>,
          </span>
          <span>
            Created At : <%= employee.createdAt %>
          </span>
        </li>
        <%}); %>
    </ul>
  </body>

  </html>